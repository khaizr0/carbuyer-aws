# AWS Production Environment

# Server Configuration
PORT=443
BASE_URL=https://carbuyer.com
SESSION_SECRET=your_production_session_secret_min_32_chars

# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# S3 Configuration
S3_BUCKET=carbuyer-production
S3_PUBLIC_URL=https://your-cloudfront-id.cloudfront.net

# JWT Configuration
JWT_SECRET=your_production_jwt_secret_min_32_chars

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=465
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_gmail_app_password

# Google Maps Configuration
GOOGLE_MAPS_EMBED_URL=https://www.google.com/maps/embed?pb=your_embed_code
